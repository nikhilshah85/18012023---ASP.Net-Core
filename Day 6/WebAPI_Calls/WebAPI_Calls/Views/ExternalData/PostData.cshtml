

<script>
    function getPostData()
    {
        fetch('https://jsonplaceholder.typicode.com/posts').then(responseType => responseType.json())
                                                           .then(postResultData => {
                                                            //  alert('Success !! - We got the data');
                                                             // console.log(postResultData);

                                                             for(var i=0;i<postResultData.length;i++)
                                                             {
                                                                 $("table").append("<tr>" +
                                                                    "<td>" + postResultData[i].id + "</td>" +
                                                                    "<td>" + postResultData[i].userId + "</td>" +
                                                                    "<td>" + postResultData[i].title + "</td>" +
                                                                    "<td>" + postResultData[i].body + "</td>" +                                                                
                                                                 "</tr>")
                                                             }
                                                           })
                                                           .catch(errDetails => {
                                                               alert('Error !!');
                                                           })
    }
</script>

<button class="btn btn-primary" onclick="getPostData()"> Get Post Data </button>
<table class="table table-bordered"></table>